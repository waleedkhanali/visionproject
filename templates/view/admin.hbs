   <script src="/socket.io/socket.io.js"></script>
    <script>
         const socket = io();
        socket.on("service",function(data){
            var parse_data = JSON.parse(data);
            console.log(parse_data);
            var notification = document.getElementById("notification");
            var children = notification.children;
             var a = document.createElement("a");
            if(children == 1){
               
                var h6 = document.createElement("h6");
                var small = document.createElement("small");
                a.className = "dropdown-item";
                h6.className = "fw-normal mb-0";
                h6.innerHTML = parse_data.mentor_username;
                small.innerHTML = "A new mentor fill a form";
                notification.appendChild(a);
                a.prepend(h6);
                a.prepend(small);
            }else{
                var h6 = document.createElement("h6");
                var small = document.createElement("small");
                var hr = document.createElement("hr");
                a.className = "dropdown-item";
                h6.className = "fw-normal mb-0";
                h6.innerHTML = parse_data.mentor_username;
                  small.innerHTML = "A new mentor fill a form";
                notification.prepend(a);
                notification.prepend(hr);
                a.appendChild(h6);
                a.appendChild(small);

            }
            a.addEventListener("click",admin_request)
        
        })
        </script>
